          <DbSource ConnectionRef="rpitst_projectConnectionString (Settings)" DbObjectName="" DbObjectType="Unknown" GenerateShortCommands="true" GeneratorSourceName="ScalarQuery" MethodsParameterType="CLR" Modifier="Public" Name="ScalarQuery" QueryType="Scalar" ScalarCallRetval="System.Object, mscorlib, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" UseOptimisticConcurrency="true" UserGetMethodName="GetDataBy" UserSourceName="ScalarQuery">
            <SelectCommand>
              <DbCommand CommandType="Text" ModifiedByUser="true">
                <CommandText>DECLARE @levels NVARCHAR(100) = 'rpitst-tveto-Level03' DECLARE @specialtys NVARCHAR(100) = 'rpitst-tveto-HDAUT01' DECLARE @years NVARCHAR(100) = 'rpitst-tveto-year-01' DECLARE @semesters NVARCHAR(100) 
= 'rpitst-tveto-Sem-001' DECLARE @academics NVARCHAR(100) = '2021-2022' DECLARE @cols NVARCHAR(4000) SELECT @cols = ISNULL(@cols + ',', '') + QUOTENAME(subject)
                                                                                                                                                                                                                               FROM      (SELECT DISTINCT subject
                                                                                                                                                                                                                                                  FROM      scores
                                                                                                                                                                                                                                                  WHERE   years = @years AND level = @levels AND specialty = @specialtys) AS Course DECLARE @qry NVARCHAR(4000)
SET          @qry = N'SELECT distinct nameen,namekh,gender,dob,[level],specialty,semester,years,batch,academic
                  ,' + @cols + '
                  ,Total
				  ,Average
				  ,Ranks
				  ,RANK () OVER (ORDER BY Average DESC) Mention
						
            FROM (SELECT s.nameen,s.namekh,s.gender,s.dob,l.nameen as level,sp.nameen as specialty,sm.nameen as semester,y.nameen as years,b.nameen as batch,academic
                         ,subject
                         , mark
                         ,(SELECT SUM(tsm.mark) FROM scores tsm where tsm.nameen = s.nameen) as Total
						,(SELECT round((SUM(tsm.mark)/count(tsm.subject)),2) FROM scores tsm where tsm.nameen = s.nameen ) as Average
						,(SELECT 
						CASE 
						WHEN (SUM(tsm.mark)/count(tsm.subject)) &gt;= 90 AND (SUM(tsm.mark)/count(tsm.subject)) &lt;=100  Then ''A'' 
						WHEN (SUM(tsm.mark)/count(tsm.subject)) &gt;= 85 AND (SUM(tsm.mark)/count(tsm.subject)) &lt;90  Then ''B+''
						WHEN (SUM(tsm.mark)/count(tsm.subject)) &gt;= 80 AND (SUM(tsm.mark)/count(tsm.subject)) &lt;85  Then ''B''
						WHEN (SUM(tsm.mark)/count(tsm.subject)) &gt;= 75 AND (SUM(tsm.mark)/count(tsm.subject)) &lt;80  Then ''C+''
						WHEN (SUM(tsm.mark)/count(tsm.subject)) &gt;= 70 AND (SUM(tsm.mark)/count(tsm.subject)) &lt;75  Then ''C''
						WHEN (SUM(tsm.mark)/count(tsm.subject)) &gt;= 65 AND (SUM(tsm.mark)/count(tsm.subject)) &lt;70  Then ''D''
						WHEN (SUM(tsm.mark)/count(tsm.subject)) &gt;= 60 AND (SUM(tsm.mark)/count(tsm.subject)) &lt;65  Then ''E''
						WHEN (SUM(tsm.mark)/count(tsm.subject)) &gt;= 0 AND (SUM(tsm.mark)/count(tsm.subject)) &lt;60  Then ''F''
						Else ''None in Rank'' END

						FROM scores tsm where tsm.nameen = s.nameen ) as Ranks
						
                 FROM scores AS s 
				 INNER JOIN levels l ON l.id = s.level
				 INNER JOIN specialtys sp ON sp.id = s.specialty
				 INNER JOIN years y ON y.id = s.years
				 INNER JOIN semesters sm ON sm.id = s.semester
				 INNER JOIN batchs b ON b.id = s.batch
				 
				 WHERE	s.level ='''
                   + @levels + ''' 
				 AND s.specialty =''' + @specialtys + '''
				 AND s.years =''' + @years + '''
				 AND s.semester =''' + @semesters + '''
				 AND s.academic =''' + @academics + ''' 
				) p 
				PIVOT ( MAX(mark)  FOR subject IN (' + @cols + ')) AS Pvt
				ORDER BY Average Desc
			'
                   /*select @qry					*/ EXEC sp_executesql @qry</CommandText>
                <Parameters />
              </DbCommand>
            </SelectCommand>
          </DbSource>
